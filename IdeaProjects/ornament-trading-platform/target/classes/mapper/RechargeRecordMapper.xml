<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.electronic.boot.mapper.RechargeRecordMapper">

    <resultMap id="BaseResultMap" type="com.electronic.boot.bean.RechargeRecord">
            <id property="serialNumber" column="serial_number" jdbcType="VARCHAR"/>
            <result property="rechargeAmount" column="recharge_amount" jdbcType="DECIMAL"/>
            <result property="paymentAccount" column="payment_account" jdbcType="VARCHAR"/>
            <result property="rechargeStatus" column="recharge_status" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="userId" column="user_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        serial_number,recharge_amount,payment_account,
        recharge_status,create_time,user_id
    </sql>
</mapper>
